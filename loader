local isClicked = false
-- Создаем экранное GUI
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

local button = Instance.new("TextButton")
button.Parent = gui -- Родительский объект кнопки (может быть изменен)
button.Size = UDim2.new(0, 50, 0, 50) -- Размер кнопки
button.Position = UDim2.new(0.5, 400, 0.5, -300) -- Позиция кнопки
button.Text = "start" -- Текст на кнопке
button.MouseButton1Click:Connect(function()
if not isClicked then
 
print("Кнопка была нажата")

loadstring(game:HttpGet("https://raw.githubusercontent.com/IceMael7/NewIceHub/main/Brookhaven"))() 
wait(30)

loadstring(game:HttpGet("https://raw.githubusercontent.com/qweaad1/qwe/main/NEWlevel1"))()
 button.Visible = false
isClicked = true
end
end)


